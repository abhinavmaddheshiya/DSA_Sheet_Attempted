class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int freq=0;
        int ans=nums[0];
        for(int i=0;i<nums.size();i++){
            if(freq==0)ans=nums[i];
            if(ans==nums[i])freq++;
            else
            freq-=1;
        }
        return ans;
    }
};
